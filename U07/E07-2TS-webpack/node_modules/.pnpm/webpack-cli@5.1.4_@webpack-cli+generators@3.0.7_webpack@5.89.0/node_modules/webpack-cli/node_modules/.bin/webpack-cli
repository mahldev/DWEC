#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules/webpack-cli/bin/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules/webpack-cli/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules/webpack-cli/bin/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules/webpack-cli/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/webpack-cli@5.1.4_@webpack-cli+generators@3.0.7_webpack@5.89.0/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
