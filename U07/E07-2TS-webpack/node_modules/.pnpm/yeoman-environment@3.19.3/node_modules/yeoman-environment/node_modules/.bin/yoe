#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules/yeoman-environment/cli/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules/yeoman-environment/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules/yeoman-environment/cli/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules/yeoman-environment/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/yeoman-environment@3.19.3/node_modules:/home/mahl/dev/projects/DWEC/U07/E07-2TS-webpack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.js" "$@"
else
  exec node  "$basedir/../../cli/index.js" "$@"
fi
